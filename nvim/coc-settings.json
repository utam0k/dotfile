{
  "languageserver": {
    "terraform": {
        "command": "terraform-ls",
        "args": ["serve"],
        "filetypes": [
            "terraform",
            "tf"
        ],
        "initializationOptions": {},
        "settings": {}
    },
  "coc.preferences.formatOnSaveFiletypes": [
     "go",
     "rust"
  ],
  "coc_diagnostic_disable": 1,
  "rust-analyzer": {
      "checkOnSave.command": "clippy",
      "cargo.allFeatures": true,
      "cargo.features": "all",
      "check.overrideCommand": [
        "cargo",
        "clippy",
        "--message-format=json",
        "--all-targets",
        "--all-features",
        "--",
        "-Dwarnings"
      ]
  },
  "yaml.schemas": {
      "kubernetes": "/*.yaml"
  },
  "go.goplsOptions": {
    "completeUnimported": true
  }
}
